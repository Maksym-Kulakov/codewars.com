
public class Kata38 {
	  public static boolean isTriangle(int a, int b, int c){
		    boolean res = false;
		    
		    if (a < b + c && b < a + c && c < a +b){
		    	res = true;
		    }
		    
		    
		    return res;
	  }
	  
	  public static void main(String[] args) {
		System.out.println(Kata38.isTriangle(2, 4, 4));
	}
	  
}
