
public class Kata17 {
	 
		public static long findNextSquare(long sq) {
	     
		long res = -1;
		
		long x = 1;
		
		while (x * x <= sq) {
			if (x * x <= 0) {
				break;
			}
			x++;	
		}
		
		if((sq % x-1 == 0)) {
			res = x * x;
		}
		
		
		
			
	      return res; 
	  
		}	
	      
		public static void main(String[] args) {
			System.out.println(Kata17.findNextSquare(15241383936L));
		}
}
