	
public class Kata56 {
	
	public static int[] countTheMonkeys(int count) {
		
		int [] array = new int [count];
		
		for (int i = 0; i < count; i++) {
			array[i] = i+1;
		}  
	
		return array;
	}
	
	public static void main(String[] args) {
		
		 
		 for(int x : Kata56.countTheMonkeys(2)) {
			 System.out.print(x);
		 }
		 
		
	}
	
}
