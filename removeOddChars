
public class Kata52 {
	 public static String removeOddChars(String originalString) {
		 String res = "";
		 for (int i = 0; i < originalString.length(); i+=2) {
			 res += originalString.substring(i, i+1);
		 }
		 return res;
	 }
	 
	 public static void main(String[] args) {
		System.out.println(Kata52.removeOddChars("qwqwqwqw"));
	}
}
