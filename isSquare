
public class Kata42 {
    
	    public static boolean isSquare(int n) {        
	    	boolean res = false;
	    	
	    	if (n == 0) {
	    		res = true;
	    	} else {
	    		
	    	
	    	int temp = 0;
	    	while(temp * temp <= n) {
	    		temp++;
	    		if(temp * temp  == n) {
	    			res = true;
	    			break;
	    		}
	    		
	    	}
	    
	    	
	   }
	    	return res;  
	    }
	    
	    public static void main(String[] args) {
			System.out.println(Kata42.isSquare(9));
		}
	 
}
